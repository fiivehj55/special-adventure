<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="dbconfig.txt"/>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
	 	<typeAlias type="pojo.web.dto.AdminDetail" alias="AdminDetail"/>
	 	<typeAlias type="pojo.web.dto.CustomerDetail" alias="CustomerDetail"/>
	 	<typeAlias type="pojo.web.dto.House" alias="House"/>
	 	<typeAlias type="pojo.web.dto.Member" alias="Member"/>
	 	<typeAlias type="pojo.web.dto.Qeustion" alias="Qeustion"/>
	 	<typeAlias type="pojo.web.dto.RegisterDetail" alias="RegisterDetail"/>
	 	<typeAlias type="pojo.web.dto.RegisterHouse" alias="RegisterHouse"/>
	 	<typeAlias type="pojo.web.dto.Report" alias="Report"/>
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<!-- <mapper resource="mappers/EmpMapper.xml"/> -->
	</mappers>
</configuration>